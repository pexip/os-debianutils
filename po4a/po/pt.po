msgstr "savelog - grava um ficheiro de relatório (log)"

# NOTE : -l en double
# type: Plain text
#. type: Plain text
#: ../savelog.8:9
msgid ""
"B<savelog> [-m mode] [-u user] [-g group] [-t] [-p] [-c cycle] [-l] [-j] [-"
"J] [-1\\ .\\|.\\ -9] [-C] [-d] [-l] [-r rolldir] [-n] [-q] [-D dateformat] "
"file ..."
msgstr ""
"B<savelog> [-m modo] [-u utilizador] [-g grupo] [-t] [-p] [-c ciclo] [-l] [-"
